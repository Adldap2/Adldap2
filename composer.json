{
    "name": "adldap2/adldap2",
    "type": "library",
    "description": "A PHP LDAP Library for Active Directory Manipulation",
    "keywords": [
        "active directory",
        "ldap",
        "windows"
    ],
    "license": "LGPL-2.1",
    "support": {
        "issues": "https://github.com/Adldap2/Adldap2/issues",
        "source": "https://github.com/Adldap2/Adldap2"
    },
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.26",
        "ext-ldap": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*",
        "mockery/mockery": "0.9.*"
    },
    "archive": {
        "exclude": ["/examples", "/tests"]
    },
    "autoload": {
        "psr-4": {
            "Adldap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adldap\\Tests\\": "tests/"
        }
    }
}
